<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="com.jh.dao.IDao">
<!-- 로그인/회원가입 -->
	<insert id="MemberJoin">
		INSERT INTO join(email, name, pw) VALUES (#{param1}, #{param2}, #{param3})
	</insert>
	
	<select id="MemberEmail" resultType="com.jh.dto.MemberDto">
		select * from join where email=#{param1}
	</select>
	
	<update id="MemberJoin2">
		UPDATE join SET RGROUP=#{param1} WHERE email=#{param2}
	</update>

<!-- 연구노트 -->
	<insert id="writeReport">
		INSERT INTO report(title,projectid,writer,contents,leadercheck)  VALUES (#{param1}, #{param2}, #{param3}, #{param4}, #{param5})
	</insert>


</mapper>